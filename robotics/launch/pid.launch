<launch>
    <node name="IR_PID" pkg="pid" type="controller" output="screen" respawn="true">
      <param name="Kp" value="10.0" />
      <param name="Ki" value="5.0" />
      <param name="Kd" value="5.0" />
      <param name="upper_limit" value="1500" />
      <param name="lower_limit" value="-1500" />
      <param name="windup_limit" value="10" />
      <param name="cutoff_frequency" value="20" />
      <param name="max_loop_frequency" value="100.0" />
      <param name="min_loop_frequency" value="100.0" />
    </node>


     <node name="setPoint_node" pkg="robotics" type="setPoint.py" respawn="true" output="screen">
     </node>


    <node name="pololuControl_node" pkg="robotics" type="pololuControl.py" respawn="true" output="screen">
    </node>


    <!--node name="rqt_plot" pkg="rqt_plot" type="rqt_plot"
    args="/control_effort/data /state/data /setpoint/data" />

    <node name="rqt_reconfigure" pkg="rqt_reconfigure" type="rqt_reconfigure" />

    <node name="rqt_robot_monitor" pkg="rqt_robot_monitor" type="rqt_robot_monitor" /-->
</launch>
